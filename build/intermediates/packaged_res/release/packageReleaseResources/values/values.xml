<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#FF000000</color>
    <color name="white">#FFFFFFFF</color>
    <style name="CircularIcon" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
        <item name="strokeWidth">1dp</item>
        <item name="strokeColor">#FFFFFF</item>
    </style>
    <declare-styleable name="CustomToolbar">
        <attr format="reference" name="wifiIcon"/>
        <attr format="reference" name="bluetoothIcon"/>
        <attr format="reference" name="drowsinessIcon"/>
        <attr format="reference" name="settingsIcon"/>
        <attr format="boolean" name="showWifiIcon"/>
        <attr format="boolean" name="showBluetoothIcon"/>
        <attr format="boolean" name="showDrowsinessIcon"/>
        <attr format="boolean" name="showSettingsIcon"/>
        <attr format="boolean" name="enableWifiClick"/>
        <attr format="boolean" name="enableBluetoothClick"/>
        <attr format="boolean" name="enableDrowsinessClick"/>
        <attr format="boolean" name="enableSettingsClick"/>
    </declare-styleable>
</resources>