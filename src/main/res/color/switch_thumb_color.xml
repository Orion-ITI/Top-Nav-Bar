<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Checked + Pressed -->
    <item android:state_checked="true" android:state_pressed="true" android:color="#004151"/>
    <!-- Unchecked + Pressed -->
    <item android:state_checked="false" android:state_pressed="true" android:color="#163546"/>
    <!-- Checked -->
    <item android:state_checked="true" android:color="#0094ba"/>
    <!-- Default (unchecked) -->
    <item android:color="#808080"/>
</selector>
