<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Checked + Pressed -->
    <item android:state_checked="true" android:state_pressed="true" android:color="#006D88"/>
    <!-- Unchecked + Pressed -->
    <item android:state_checked="false" android:state_pressed="true" android:color="#3A4F60"/>
    <!-- Checked -->
    <item android:state_checked="true" android:color="#33B9D6"/>
    <!-- Default (unchecked) -->
    <item android:color="#B0B0B0"/>
</selector>
